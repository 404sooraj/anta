# AWS Bedrock - Titan Embeddings G1 - Text (same as server getCallInsights)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_SESSION_TOKEN=your_aws_session_token
AWS_REGION=us-west-2
BEDROCK_EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1:0

# Pinecone (same indexes as server getCallInsights)
# Indexes are created automatically if missing (serverless, dimension 1536).
# Names are normalized for Pinecone: underscores → hyphens (e.g. call_scenarios → call-scenarios).
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_REGION=us-east-1
PINECONE_INDEX_CALL_SCENARIOS=call_scenarios
PINECONE_INDEX_RESPONSE_PATTERNS=response_patterns
PINECONE_INDEX_POLICY_KNOWLEDGE=policy_knowledge
